<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <link rel="stylesheet" href="/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/css/main.css" />

    <script src="/js/jquery.min.js"></script>
    <script src="/js/scripts.js"></script>

    <title>Document</title>
  </head>
  <body class="container">
    <h1>Code</h1>

    <div class="row">
      <div class="col-md-12">
        <button rel="body" class="btn-insert btn btn-default btn-outline" type="submit">
          Inserir corpo da mensagem
        </button>
        <button rel="input" class="btn-insert btn btn-default btn-outline" type="submit">
          Inserir campo
        </button>
        <button rel="button" class="btn-insert btn btn-default btn-outline" type="submit">
          Inserir bot√£o
        </button>
      </div>
    </div>

    <div class="row">    
      <div class="col-md-6">
        <div class="wrapper">
          <a href="#" class="content-img">
            <img src="images/ilu.png" alt="ilu">
          </a>
        </div>
        <textarea id="code" rows="20" class="form-control content-code"></textarea>
        <span class="input-group-btn">
          <button id="renderCode" class="btn btn-info bnt-action" type="button">
            Renderizar
          </button>
        </span>
      </div>
      <div class="col-md-6">
        <div class="col-md-6">
          <div class="layout-container-cel">              
            <div class="detail"></div>
              <div class="layout-content-cel">
                <h3 class="title">Celular 1</h3>
                <div id="chat1"></div>
              </div> 
            <div class="circle"></div>            
          </div>         
        </div>
        <div class="col-md-6">
          <div class="layout-container-cel"> 
            <div class="detail"></div>
            <div class="layout-content-cel">          
              <h3 class="title">Celular 2</h3>
              <div id="chat2">
            <textarea
              id="messages"
              disabled
              class="form-control"
              rows="4"
            ></textarea>
            <div class="input-group">
              <input
                type="text"
                class="form-control"
                placeholder="Digite sua mensagem"
              />
              <span class="input-group-btn">
                <button class="btn btn-success" type="button">Enviar</button>
              </span>
            </div>
              </div>
            </div>
            <div class="circle"></div>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
